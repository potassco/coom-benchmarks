product {
			1..30        BigTable bigTables
			1..30        SmallTable smallTables
			1-500    num   totalPlaces
			}
			structure BigTable {
				3..5 Chair chairs
			}
			structure SmallTable {
				1..2 Chair chairs
			}
			structure Chair {
			}

			behavior {
				require totalPlaces = 100

				imply totalPlaces = count(bigTables.chairs) + count(smallTables.chairs)
			}
