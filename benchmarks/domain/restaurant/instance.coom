product {
			1..NUMTABLE	BigTable bigTables
			1..NUMTABLE	SmallTable smallTables
			num	1-500	totalPlaces
			}
			structure BigTable {
				3..5 Chair chairs
			}
			structure SmallTable {
				1..2 Chair chairs
			}
			structure Chair {
			}

			behavior {
				require totalPlaces = TOTALPLACES

				imply totalPlaces = count(bigTables.chairs) + count(smallTables.chairs)
			}
