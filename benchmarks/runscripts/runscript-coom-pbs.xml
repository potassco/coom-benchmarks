<runscript output="output">

	<machine name="komputer" cpu="24x8xE5520@2.27GHz" memory="24GB"/>

	<config name="seq-generic" template="templates/seq-generic.sh"/>

	<system name="coomsolver" version="0.1" measures="clasp" config="seq-generic">
		<setting name="clingo" cmdline="--models=0 -q --solver clingo" tag="clingo" />
		<setting name="fclingo" cmdline="--models=0 -q --solver fclingo" tag="fclingo" />
    </system>

	<pbsjob name="pbs-gen" timeout="600" runs="1" script_mode="timeout" walltime="15:00:00" cpt="4"/>

	<benchmark name="kids-bike">
		<folder path="../domain/kids-bike/instances">
		</folder>
	</benchmark>

	<benchmark name="city-bike-fleet">
		<folder path="../domain/city-bike-fleet/instances">
		</folder>
	</benchmark>

	<benchmark name="travel-bike-fleet">
		<folder path="../domain/travel-bike-fleet/instances">
		</folder>
	</benchmark>

	<benchmark name="restaurant">
		<folder path="../domain/restaurant/instances">
		</folder>
	</benchmark>


	<project name="kids-bike-clingo" job="pbs-gen">
		<runtag machine="komputer" benchmark="kids-bike" tag="clingo"/>
	</project>

	<project name="kids-bike-fclingo" job="pbs-gen">
		<runtag machine="komputer" benchmark="kids-bike" tag="fclingo"/>
	</project>

	<project name="city-bike-fleet-clingo" job="pbs-gen">
		<runtag machine="komputer" benchmark="city-bike-fleet" tag="clingo"/>
	</project>

	<project name="city-bike-fleet-fclingo" job="pbs-gen">
		<runtag machine="komputer" benchmark="city-bike-fleet" tag="fclingo"/>
	</project>

	<project name="travel-bike-fleet-clingo" job="pbs-gen">
		<runtag machine="komputer" benchmark="travel-bike-fleet" tag="clingo"/>
	</project>

	<project name="travel-bike-fleet-fclingo" job="pbs-gen">
		<runtag machine="komputer" benchmark="travel-bike-fleet" tag="fclingo"/>
	</project>

	<project name="restaurant-clingo" job="pbs-gen">
		<runtag machine="komputer" benchmark="restaurant" tag="clingo"/>
	</project>

	<project name="restaurant-fclingo" job="pbs-gen">
		<runtag machine="komputer" benchmark="restaurant" tag="fclingo"/>
	</project>
</runscript>

