product {
			1..15        BigTable bigTables
			1..15        SmallTable smallTables
			1..500    num   totalPlaces
			}
			structure BigTable {
				3..5 Chair chairs
			}
			structure SmallTable {
				1..2 Chair chairs
			}
			structure Chair {
			}

			behavior {
				require totalPlaces = 80

				imply totalPlaces = count(bigTables.chairs) + count(smallTables.chairs)
			}
